@incollection{Patry.2012,
 abstract = {Herbart bezeichnet den {\dq}P{\"a}dagogischen Takt{\dq} als Zwischenglied zwischen Theorie und Praxis. Ausgehend von den (wenigen) theoretischen {\"U}berlegungen zum Takt und Erkenntnissen aus anderen Diskussionen sollen Elemente zu einer {\dq}Theorie des Taktes{\dq} entwickelt werden. Dabei wird zwischen Theorien1 (Theorien, die mittels Takt in praktisches Tun zu {\dq}{\"u}bersetzen{\dq} sind) und Theorien2 (theoretische Aussagen {\"u}ber den Takt) unterschieden. Praxis wird als Handeln bestimmt. Theorien1 k{\"o}nnen dann nur handlungswirksam werden, wenn sie die Praktikerin oder der Praktiker in das System handlungsleitender subjektiver Theorien aufgenommen hat. F{\"u}r das taktvolle Handeln sind sodann folgende Elemente zu ber{\"u}cksichtigen: Komplexit{\"a}t (Theorien1 vereinfachen die Ph{\"a}nomene zwangsl{\"a}ufig, in der Praxis muss aber der ganzen Komplexit{\"a}t Rechnung getragen werden), damit zusammenh{\"a}ngend die Allgemeinheits-Konkretheits-Antinomie ({\dq}Je allgemeiner eine Aussage ist, desto weniger konkret kann sie sein{\dq}), Zielvielfalt (Praktikerinnen und Praktiker verfolgen fast immer mehrere, auch entgegengesetzte Ziele simultan), die Theorienvielfalt (bezogen auf Theorien1; Praktikerinnen und Praktiker verwenden fast immer mehrere Theorien1 simultan) und Situationsspezifit{\"a}t (in unterschiedlichen Situationen ist meist unterschiedliches Verhalten zielf{\"u}hrend). Hinzu kommen ethische {\"U}berlegungen. Auf diesen Elementen aufbauend wird ein Modell der Dynamik des P{\"a}dagogischen Taktes entwickelt, welches so konzipiert ist, dass die Praktikerin oder der Praktiker eine substanzielle kreative Leistung zu erbringen hat - in dieser liegt ein wesentlicher Teil ihrer oder seiner Verantwortung. (DIPF/Orig.).},
 author = {Patry, Jean-Luc},
 title = {Der P{\"a}dagogische Takt},
 keywords = {Begriff;Definition;Herbart, Johann Friedrich;Lehrer;Menschenw{\"u}rde;P{\"a}dagogik;P{\"a}dagogisches Handeln;Pers{\"o}nlichkeitsmerkmal;Praxis;Subjektive Theorie;Taktgef{\"u}hl;Theorie;Verantwortung},
 editor = {Rahm, Sibylle and Nerowski, Christian},
