@article{Blomberg.2013,
 abstract = {This article provides a research synthesis on the use of video in pre-service teacher education. Common ideas and evidences concerning the use of video in pre-service teacher education are reviewed. Based on the state-of-the-art in using video, five research-based heuristics are derived. Research findings of a number of studies are further used to illustrate the specification of heuristics. Specifically, a set of rules of thumb about when, how, and why to use video is presented to clarify the strengths and limitations of video as a medium to support pre-service teacher learning.},
 author = {Blomberg, Geraldine and Renkl, Alexander and Sherin, Miriam Gamoran and Borko, Hilda and Seidel, Tina},
