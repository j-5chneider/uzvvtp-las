@article{Vargha.2000,
 abstract = {McGraw and Wong (1992) described an appealing index of effect size, called {\dq}CL{\dq}, which measures the difference between two populations in terms of the probability that a score sampled at random from the first population will be greater than a score sampled at random from the second. McGraw and Wong introduced this {\dq}common language effect size statistic{\dq} for normal distributions and then proposed an approximate estimation for any continuous distribution. In addition, they generalized {\dq}CL{\dq} to the n-group case, the correlated samples case, and the discrete values case. In the current paper a different generalization of {\dq}CL{\dq} called the A measure of stochastic superiority, is proposed, which may be directly applied for any discrete or continuous variable that is at least ordinally scaled. Exact methods for point and interval estimation as well as the significance tests of the A = .5 hypothesis are provided. New generalizations of {\dq}CL{\dq} are provided for the multi-group and correlated samples cases.},
 author = {Vargha, Andr{\'a}s and Delaney, Harold D.},
 year = {2000},
 title = {A Critique and Improvement of the {\dq}CL{\dq} Common Language Effect Size Statistics of McGraw and Wong},
 pages = {101--132},
